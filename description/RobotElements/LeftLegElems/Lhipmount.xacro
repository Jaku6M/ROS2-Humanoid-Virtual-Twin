<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >
  <!-- LEFT HIP MOUNT-->
  <joint name="LTz" type="revolute">
    <!--Origin defines translation child frame relative to parent frame. Translation is performed in parent frame.-->
    <origin xyz="0	40e-3	-150.065e-3" rpy="0 0 0"/>
    <parent link="Trunk"/>
    <child link="Left_HipMount"/>
    <axis xyz="0 0 1.0"/>
    <limit lower="${-pi/2}" upper="${pi/2}" effort="100" velocity="100"/>
  </joint>  
  <link name="Left_HipMount">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find ros2_humanoid_virtual_twin)/meshes/Left-HipMount.stl"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find ros2_humanoid_virtual_twin)/meshes/Rgiht-HipMount-Collision.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="-14.306e-3	0	24.769e-3" rpy="0 0 0"/>
      <mass value="0.038"/>
      <inertia ixx="11.743e-6" iyy="61.298e-6" izz="58.734e-6" ixy="0" ixz="4.429e-6"  iyz="0" />
    </inertial>    
  </link> 
  <gazebo reference="Left_HipMount">
      <material>Gazebo/Grey</material>
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <selfCollide>true</selfCollide>      
  </gazebo>   
</robot>
