<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >
    <joint name="LFy" type="revolute">
      <origin xyz="0 0 -128e-3" rpy="0 0 0"/>
      <parent link="Left_Shank"/>
      <child link="Left_Ankle"/>
      <axis xyz="0 1.0 0"/>
      <limit lower="${-pi/2}" upper="${pi/2}" effort="100" velocity="100"/>
    </joint>
	<link name="Left_Ankle">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find ros2_humanoid_virtual_twin)/meshes/Left-Ankle.stl"/>
        </geometry>
        <material name="grey"/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <mesh filename="file://$(find ros2_humanoid_virtual_twin)/meshes/Left-Ankle-Collision.stl"/>
        </geometry>
      </collision>
      <inertial>
        <origin xyz="-21.137e-3	0.17e-3	14.739e-3" rpy="0 0 0"/>
        <mass value="0.351"/>
        <inertia ixx="135.823e-6" iyy="317.524e-6" izz="266.663e-6" ixy="-1.265e-6" ixz="4.25e-6"  iyz="2.029e-6" />
      </inertial>      
    </link> 
    <gazebo reference="Left_Ankle">
      <material>Gazebo/Grey</material>
        <mu1 value="200.0"/>
        <mu2 value="100.0"/>
        <kp value="10000000.0" />
        <kd value="1.0" />
        <selfCollide>true</selfCollide>
    </gazebo> 
</robot>
