<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >
    <joint name="LAx" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="Left_ArmMounting"/>
        <child link="Left_Arm"/>
        <axis xyz="1.0 0 0"/>
        <limit lower="${-pi/2}" upper="${pi/2}" effort="100" velocity="100"/>
    </joint>
    <link name="Left_Arm">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ros2_humanoid_virtual_twin)/meshes/Left-Arm.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="grey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find ros2_humanoid_virtual_twin)/meshes/Left-Arm-Collision.stl"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0.152e-3 -0.087e-3 -49.855e-3" rpy="0 0 0"/>
            <mass value="0.165"/>
            <inertia ixx="269.772e-6" iyy="269.138e-6" izz="29.837e-6" ixy="-0.02e-6" ixz="-1.454e-6"  iyz="-0.269e-6"/>
        </inertial>
    </link>
    <gazebo reference="Left_Arm">
        <material>Gazebo/Grey</material>
    </gazebo>
</robot>
  